{
  "name": "agoralia-frontend",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "engines": {
    "node": ">=20.10.0 <23.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview --port 4173",
    "prebuild": "npm run audit:ci",
    "audit:ci": "npm audit --omit=dev --audit-level=high || true",
    "audit:fix": "npm audit fix --omit=dev || true",
    "audit:full": "npm audit",
    "test:demo-mode": "npx playwright test tests/demo-mode.spec.js",
    "test:numbers": "npx playwright test tests/numbers.spec.js",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "i18n:sync": "python ../scripts/i18n_sync.py",
    "i18n:validate": "python ../scripts/i18n_validate.py",
    "i18n:scan": "python ../scripts/i18n_scan_code.py",
    "i18n:add-missing": "python ../scripts/i18n_add_missing_keys.py",
    "i18n:fix-ph": "python3 ../scripts/i18n_fix_placeholders.py --root frontend/locales --ns common pages auth billing app admin ui errors compliance integrations settings kb",
    "i18n:complete": "npm run i18n:scan && npm run i18n:fix-ph && npm run i18n:sync && npm run i18n:validate"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "5.2.1",
    "@playwright/test": "1.55.0",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.15",
    "@stripe/react-stripe-js": "^3.9.1",
    "@stripe/stripe-js": "^7.8.0",
    "@tanstack/react-query": "^5.85.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "i18next": "25.4.0",
    "libphonenumber-js": "^1.12.13",
    "lucide-react": "^0.462.0",
    "papaparse": "^5.5.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.3.8",
    "react-hook-form": "^7.62.0",
    "react-i18next": "15.7.1",
    "react-router-dom": "^6.23.0",
    "recharts": "3.1.2",
    "tailwind-merge": "^3.3.1",
    "zod": "^4.1.0",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.2.0",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.41",
    "tailwindcss": "^3.4.10",
    "terser": "^5.43.1",
    "vite": "^5.0.0"
  },
  "overrides": {
    "esbuild": "^0.24.2"
  }
}
